{
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng serve account-budget",
      "type": "msedge",
      "request": "launch",
      "preLaunchTask": "npm: start:account-budget",
      "url": "http://localhost:4200/account-budget",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:/*": "${webRoot}/*",
        "/./*": "${webRoot}/*",
        "/src/*": "${webRoot}/src/*",
        "/*": "*",
        "/./~/*": "${workspaceFolder}/node_modules/*"
      },
      "userDataDir": "${workspaceRoot}/out/edge",
    },
    {
      "name": "ng test",
      "type": "msedge",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:9876/debug.html"
    }
  ]
}
